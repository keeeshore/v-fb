{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for Angular",
  "scripts": {
    "start1": "webpack-dev-server --inline --progress --port 3131",
    "start": "rimraf dist && webpack-dev-server --inline --config config/webpack.dev.js --progress --environment=development --port 3131",
